def floodActive:
    location= input("Where are you", "Options: Home, Outdoors, Indoors")
    location= location.lower()
    if location != "home" or location != "outdoors" or location != "indoors":
        print("Sorry that is not a option")
        location= input("Where are you", "Options: Home, Outdoors, Indoors")
    return location
